{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [
      "es2019"
    ],
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "esnext",
    "types": ["react", "react-native"],
    "typeRoots": ["./node_modules/@types", "./types"],
    "baseUrl": "./",
    "resolveJsonModule": true,
    "allowJs": true
  },
  "extends": "expo/tsconfig.base"
}
